<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" >

    <Appenders>
        <Console name="console" target="SYSTEM_OUT">
            <PatternLayout pattern="[%5p][%d{yyyy-MM-dd HH:mm:ss,SSS}][%X{guid}][%F:%L][%t] %m%n"/>
        </Console>
   
    </Appenders>
    
    <Loggers>
        <Root level="trace">
            <AppenderRef ref="console"/>
        </Root>
    </Loggers>
</Configuration>
